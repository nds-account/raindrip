	<languages>
		<language tag="en-GB">en-GB.mod_yj_multimediabox.ini</language>
		<language tag="en-GB">en-GB.mod_yj_multimediabox.sys.ini</language>
	</languages>
	<files>
		<filename module="mod_yj_multimediabox">mod_yj_multimediabox.php</filename>
		<filename>helper.php</filename>
		<filename>index.html</filename>
		<folder>tmpl</folder>
		<folder>elements</folder>
		<folder>images</folder>
	</files>
	<config>
		<fields name="params" addfieldpath="/modules/mod_yj_multimediabox/elements">
