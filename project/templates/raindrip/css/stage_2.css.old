/* DROP KITS CIRCLE ANIMATION */
.circle .detail {
    background-color: #59C0EC;
    z-index:2;
    position:absolute;
    opacity:0;
    width:117px;
    height:117px;
    display: table;
    text-align: center;
    animation: 500ms linear 0s normal none 1 revealDetailsContainer;
    -webkit-border-radius:115px;
    -moz-border-radius:115px;
    -ms-border-radius:115px;
    -o-border-radius:115px;
    border-radius:115px;
}

.detail span {
    display:table-cell;
    vertical-align:middle;
    padding-left:10px;
    padding-right:10px;
    font-size: 10px;
    line-height: 12px;
}

.highlight {
    width:110px;
    height:110px;
    display:block;
    position:absolute;
    top:0;
    left:0;
    border:5px solid #59c0ec;
    -webkit-border-radius:115px;
    -moz-border-radius:115px;
    -ms-border-radius:115px;
    -o-border-radius:115px;
    border-radius:115px;
    z-index:5;
    opacity:0;
    -webkit-transition-property:opacity;
    -moz-transition-property:opacity;
    -o-transition-property:opacity;
    transition-property:opacity;
    -webkit-transition-duration:600ms;
    -moz-transition-duration:600ms;
    -o-transition-duration:600ms;
    transition-duration:600ms;
}

.button-circle {
    width:117px;
    height:117px;
    -webkit-border-radius:50%;
    -moz-border-radius:50%;
    border-radius:50%;
    cursor:pointer;
    transition:all 1s ease-in;
    -webkit-transition:0 .2s ease-in-out;
    -moz-transition:all .2s ease-in-out;
    -ms-transition:all .2s ease-in-out;
    position:absolute;
    z-index:200;
}


@-moz-keyframes revealDetailsContainer {
    0% {
    background-color: rgba(89, 192, 236, 0.6);
    height: 115px;
    margin-left: -57.5px;
    margin-top: -57.5px;
    width: 115px;
}
40% {
    background-color: #59C0EC;
}
70% {
    height: 115px;
    margin-left: -57.5px;
    margin-top: -57.5px;
    width: 115px;
}
100% {
    height: 172.5px;
    margin-left: -86.25px;
    margin-top: -86.25px;
    width: 172.5px;
}
}
@-moz-keyframes revealDetailsContainer {
    0% {
